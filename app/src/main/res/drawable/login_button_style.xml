<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_enabled="false">
        <shape>
            <solid android:color="#9D9D9D" />
            <stroke android:width="1dp" android:color="#8E8E8E" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#0072E3" />
            <stroke android:width="1dp" android:color="#0066CC" />
            <corners android:radius="5dp" />
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape>
            <solid android:color="#2894FF" />
            <stroke android:width="1dp" android:color="#0080FF" />
            <corners android:radius="5dp" />
        </shape>
    </item>

</selector>